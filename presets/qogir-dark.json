  "custom_css": {
    "gtk4": "\nwindow:not(.maximized), window:not(.maximized).background,\nmenu, .menu, .context-menu,\npopover contents, popover arrow, tooltip {\n    border-radius: 5px;\n}\n\ndialog, window, .popup dialog, .popup window, .csd dialog.popup, .csd window.popup {\n  box-shadow: 0px 0px 16px rgba(0, 0, 0, 0.5), 0px 2px 8px rgba(0, 0, 0, 0.3);\n}\n.csd dialog.tooltip, .csd window.tooltip {\n  box-shadow: none;\n}\n.csd dialog.message-dialog, .csd window.message-dialog {\n  box-shadow: 0 2px 6px 1px rgba(0, 0, 0, 0.5);\n}\n\npopover contents, popover arrow {\n    border: 1px solid @dark_1;\n}\n\ntooltip {\n    background-color: @dark_3;\n}\n\nwindowcontrols>button {\n  padding: 0px;\n}\n\nwindowcontrols button image:not(:hover) {\n  background-color: transparent;\n}\n\n.navigation-sidebar, toolbarview {\n  background-color: @dark_5;\n}\n\n"
  }
